						<span style="color: #DF151A;">Bo</span><span style="color: #F4F328;">li</span><span style="color: #00DA3C;">via</span</h1>
					-->
				</div>
			</div>
			<form class="form-inline" action="/">

			<div class="row">
				<div class="col-lg-12">
					  <div class="form-group">
						    <input type="text" size="35" class="form-control input-lg" id="objeto" placeholder="Objeto Contrato">
					  </div>
					  <div class="form-group">
					  	<%= autocomplete_field_tag 'entity', '', autocomplete_entity_name_contracts_path, {:class=> "form-control input-lg", :size => 35, :placeholder => "Entidad", :id => "entidad", 'data-auto-focus' => true} %>
					  </div>
					  <div class="form-group">
							<%=  
					    	collection_select(:region, :id, Region.all, :id, :name, {:include_blank => 'TODOS'}, {:class=> "form-control input-lg"})
					    	%>
					  </div>
					  <button type="submit" class="btn btn-lg btn-theme btn-default">Buscar</button>
					  <br>
					  <h3 style="float: right;" href="">Busqueda avanzada</h3>
